{
    "name": "hanwoolderink88/router",
    "description": "router",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hanwoolderink",
            "email": "hanwoolderink@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hanwoolderink88\\Router\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.5",
        "phpstan/phpstan": "^0.12.39"
    },
    "scripts": {
        "test": [
            "@stan",
            "@unit",
            "@sniff"
        ],
        "unit": "vendor/bin/phpunit Tests",
        "unit-coverage": "vendor/bin/phpunit --coverage-html Tests/html Tests",
        "sniff": "vendor/bin/phpcs --standard=ruleset.xml src Tests",
        "sniff-fix": "vendor/bin/phpcbf --standard=ruleset.xml src Tests",
        "stan": "vendor/bin/phpstan analyse --level 6 src Tests"
    }
}
